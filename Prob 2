#include <iostream>
#include <conio.h>
#include <iomanip>
using namespace std;

float main()
{

	double mass;
	double density; 
	double volume;

		cout << "Input the mass in grams: ";
		cin >> mass;
		cout << "Input the density in grams per cubic centimeter: ";
		cin >> density;
		cout << "The volume is :";

		cout<< setprecision(3) << mass / density << endl;

	_getch();
    return 0;
}
